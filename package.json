    "start": "expo start",
    "android": "rm -rf android && expo run:android",
    "eas": "npx dotenv-cli@latest -e .env -- npx eas-cli@latest",
    "eas:secret": "yarn eas env:create --scope project --visibility=secret ",
    "eas-build-pre-install": "./pre-install",
    "build:android": "yarn version && yarn eas build --profile production --platform android",
    "build:ios": "yarn eas build --profile production --platform ios",
    "predeploy": "npx expo export -p web --output-dir dist",
    "prebuild": "expo prebuild --clean",
    "deploy": "gh-pages -d dist --cname zikr.valabji.com",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --coverage=false",
