{
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "https://zikr.valabji.com/",
  "scripts": {
    "start": "expo start",
    "android": "rm -rf android && expo run:android",
    "eas": "source .env && npx cross-env@latest EAS_PROJECT_ID=$EAS_PROJECT_ID npx eas-cli@latest",
    "eas:secret": "yarn eas env:create --scope project --visibility=secret ",
    "eas-build-pre-install": "./pre-install",
    "build:android": "yarn eas build --profile production --platform android",
    "predeploy": "npx expo export -p web --output-dir dist",
    "deploy": "gh-pages -d dist --cname zikr.valabji.com",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --coverage=false",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:ci": "jest --ci"
  },
